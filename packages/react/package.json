{
  "name": "@logoicon/react",
  "version": "1.0.1",
  "description": "react component of logoicon",
  "keywords": [
    "logo icon",
    "logo",
    "icon"
  ],
  "license": "MIT",
  "author": {
    "name": "dembilesmana",
    "email": "dembilesmana@gmail.com",
    "url": "https://dembilesmana.vercel.app"
  },
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./*": {
      "import": {
        "types": "./dist/**/*.d.ts",
        "default": "./dist/**/*.js"
      },
      "require": {
        "types": "./dist/**/*.d.cts",
        "default": "./dist/**/*.cjs"
      }
    },
    "./metadata": {
      "import": {
        "types": "./dist/metadata.d.ts",
        "default": "./dist/metadata.js"
      },
      "require": {
        "types": "./dist/metadata.d.cts",
        "default": "./dist/metadata.cjs"
      }
    }
  },
  "files": [
    "./dist",
    "./CHANGELOG.md"
  ],
  "scripts": {
    "build": "tsup",
    "generate": "tsx ./scripts/generate.ts",
    "tsc": "tsc -p ./tsconfig.json"
  },
  "dependencies": {
    "@logoicon/core": "^1.0.1"
  },
  "devDependencies": {
    "@logoicon/eslintconfig": "^1.0.1",
    "@logoicon/logger": "^1.0.1",
    "@logoicon/tsconfig": "^1.0.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
