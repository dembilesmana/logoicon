{
  "$schema": "https://turborepo.com/schema.json",
  "ui": "tui",
  "tasks": {
    "generate": {
      "cache": false,
      "outputs": [".dist/**", ".assets/**"]
    },
    "@logoicon/react#generate": {
      "dependsOn": ["^generate"],
      "cache": false,
      "inputs": ["$TURBO_DEFAULT$", "../core/.assets/**", "../core/.dist/**"],
      "outputs": [".dist/**"]
    },
    "dev": {
      "dependsOn": ["^generate"],
      "cache": false,
      "persistent": true
    }
  }
}
