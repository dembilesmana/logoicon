{
  "$schema": "https://turborepo.com/schema.json",
  "ui": "tui",
  "tasks": {
    "dev": {
      "cache": false,
      "dependsOn": ["^generate", "^build"],
      "persistent": true
    },
    "build": {
      "dependsOn": ["generate", "^build"],
      "outputs": ["dist/**"]
    },
    "generate": {
      "cache": false,
      "dependsOn": ["^generate"],
      "outputs": [".dist/**"]
    },
    "@logoicon/core#generate": {
      "outputs": [".assets/**"]
    },
    "@logoicon/react#generate": {
      "inputs": ["$TURBO_DEFAULT$", "../core/.assets/**", "../core/.dist/**"]
    },
    "web#build": {
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": [".next/**", "!.next/cache/**"]
    },
    "lint": {
      "dependsOn": ["^lint"]
    }
  }
}
